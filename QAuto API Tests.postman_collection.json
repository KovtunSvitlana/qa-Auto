							"                \"password\": { \"type\": \"string\" },",
							"                \"repeatPassword\": { \"type\": \"string\" }",
							"            },",
							"            \"required\": [\"name\", \"lastName\", \"email\", \"password\", \"repeatPassword\"]",
							"        }",
							"    }",
							"};",
